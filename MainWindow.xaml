<Window x:Class="EmediaWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmediaWPF"
        mc:Ignorable="d"
        Title="PngParser" Height="350" Width="600" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="PermutationAlgButton" Content="Wybierz plik" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" Width="122" Height="30" Click="LoadFile_Click" Margin="13,5,162,29"/>
        <Image x:Name="MainImage" HorizontalAlignment="Left" Height="274" Margin="6,42,0,0" Grid.RowSpan="5" VerticalAlignment="Top" Width="287"/>
        <Image x:Name="FourierImage" HorizontalAlignment="Left" Height="274" Margin="6,42,0,0" Grid.RowSpan="5" VerticalAlignment="Top" Width="287" Grid.Column="1"/>
    </Grid>
</Window>
